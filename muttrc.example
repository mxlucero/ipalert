set realname = "First Last"
set from = "<gmail_username>@gmail.com"
set use_from = yes
set envelope_from = yes


set smtp_url = "smtps://<gmail_username>@gmail.com@smtp.gmail.com:465/"
set smtp_pass = "<app_specific_password>"
